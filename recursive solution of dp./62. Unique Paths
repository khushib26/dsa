class Solution {
public:
    int count=0;
    int uniquePaths(int m, int n) {
        int a,b;
        if(m==1 && n==1)
        {
            count++;
        }
        else
        {
            if(m>1)
            {
               a=uniquePaths(m-1,n);
            }
            if(n>1)
            {
                b=uniquePaths(m,n-1);
            }
        }
     
        return count;
    }
};
